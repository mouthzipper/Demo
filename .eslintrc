{
  "rules": {
    "camelcase": 0,
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "no-console": [
      2,
      {
        allow: [
          "log",
          "warning",
          "error"
        ]
      }
    ],
    "no-param-reassign": [
      2,
      {
        "props": false
      }
    ],
    "space-before-function-paren": [
      2,
      {
        "anonymous": "always",
        "named": "never"
      }
    ],
    "no-use-before-define": [
      2,
      "nofunc"
    ],
    "comma-dangle": [
      0
    ],
    "no-underscore-dangle": [
      0
    ],
    "no-nested-ternary": [
      0
    ],
    "no-plusplus": [
      0
    ],
    "import/extensions": [
      0
    ],
    "import/no-unresolved": [
      0
    ]
  },
  "env": {
    "node": true,
    "mocha": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"]
}
